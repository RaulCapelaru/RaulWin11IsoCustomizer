<!-- RaulWin11 Advanced Tweaks - FirstLogonCommands -->
<!-- Adaugă această secțiune în <FirstLogonCommands> din autounattend.xml -->

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate" /f</CommandLine>
    <Description>Create Windows Update registry keys</Description>
    <Order>1</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU" /f</CommandLine>
    <Description>Create Windows Update AU keys</Description>
    <Order>2</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate" /v DeferFeatureUpdates /t REG_DWORD /d 1 /f</CommandLine>
    <Description>Defer Feature Updates</Description>
    <Order>3</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate" /v DeferFeatureUpdatesPeriodInDays /t REG_DWORD /d 365 /f</CommandLine>
    <Description>Defer Feature Updates 365 days</Description>
    <Order>4</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate" /v BranchReadinessLevel /t REG_DWORD /d 32 /f</CommandLine>
    <Description>Set Branch Readiness Level</Description>
    <Order>5</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate" /v ExcludeWUDriversInQualityUpdate /t REG_DWORD /d 1 /f</CommandLine>
    <Description>Exclude driver updates</Description>
    <Order>6</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU" /v NoAutoUpdate /t REG_DWORD /d 0 /f</CommandLine>
    <Description>Enable controlled updates</Description>
    <Order>7</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU" /v AUOptions /t REG_DWORD /d 2 /f</CommandLine>
    <Description>Notify before download and install</Description>
    <Order>8</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsUpdate\AU" /v NoAutoRebootWithLoggedOnUsers /t REG_DWORD /d 1 /f</CommandLine>
    <Description>No auto reboot with users logged on</Description>
    <Order>9</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\CloudContent" /v DisableWindowsConsumerFeatures /t REG_DWORD /d 1 /f</CommandLine>
    <Description>Disable consumer features</Description>
    <Order>10</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\Appx" /v BlockNonAdminUserInstall /t REG_DWORD /d 1 /f</CommandLine>
    <Description>Block non-admin app installs</Description>
    <Order>11</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\WindowsStore" /v AutoDownload /t REG_DWORD /d 2 /f</CommandLine>
    <Description>Disable Store auto-download</Description>
    <Order>12</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\DataCollection" /v AllowTelemetry /t REG_DWORD /d 0 /f</CommandLine>
    <Description>Disable telemetry</Description>
    <Order>13</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\System" /v PublishUserActivities /t REG_DWORD /d 0 /f</CommandLine>

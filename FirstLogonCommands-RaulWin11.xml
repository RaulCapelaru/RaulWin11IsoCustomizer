    <Order>18</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\Windows Search" /v ConnectedSearchUseWeb /t REG_DWORD /d 0 /f</CommandLine>

    <Order>14</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\System" /v UploadUserActivities /t REG_DWORD /d 0 /f</CommandLine>

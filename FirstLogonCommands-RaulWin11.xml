    <Order>19</Order>
</SynchronousCommand>

<SynchronousCommand wcm:action="add">
    <CommandLine>cmd /c reg add "HKLM\SOFTWARE\Policies\Microsoft\Windows\OOBE" /v DisablePrivacyExperience /t REG_DWORD /d 1 /f</CommandLine>
